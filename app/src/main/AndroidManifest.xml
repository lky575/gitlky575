<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lky575.parkingmanager">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/park_64"
        android:label="모두의 주차"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Config"
            android:screenOrientation="portrait"/>
        <activity android:name=".aeroView"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".MyCarPosition"
            android:windowSoftInputMode="adjustPan"
            android:screenOrientation="portrait"/>
        <activity android:name=".CalculateFare"
            android:screenOrientation="portrait"/>
        <activity android:name=".MyLogActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>